{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/",
      "use": "vercel-rust"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/api"
    },
    {
      "src": "/_next/(.*)",
      "dest": "frontend/_next/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend"
    }
  ]
}