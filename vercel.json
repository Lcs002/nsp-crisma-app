{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/Cargo.toml",
      "use": "vercel-rust"
    },
    {
      "src": "frontend/next.config.ts",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/api"
    },
    {
      "src": "/(.*)",
      "dest": "frontend"
    }
  ]
}